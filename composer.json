{
    "name": "enhancedreality/laravel-locationhierarchy",
    "description": "Hierarchical locations for geo data storage",
    "type": "laravel-module",
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "EnhancedReality\\LocationHierarchy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnhancedReality\\LocationHierarchy\\Test\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Sobtchouk"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "EnhancedReality\\LocationHierarchy\\LocationServiceProvider"
            ]
        }
    }
}
